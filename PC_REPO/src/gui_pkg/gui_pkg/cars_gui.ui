    border: 2px solid #ff6f3c;
    border-radius: 10px;
    padding: 10px 20px;
    font-weight: bold;
    text-transform: uppercase;
    text-shadow: 0 0 6px #ff6f3c;
    box-shadow: 0 0 12px #ff6f3c;
}

QPushButton:hover {
    background-color: #ff6f3c;
    color: #2b0f0f;
    box-shadow: 0 0 20px #ff6f3c;
}

QPushButton:pressed {
    background-color: #ff3c1f;
    box-shadow: inset 0 0 10px #ff6f3c;
}

/* Labels */
QLabel {
    color: #ffcc99;
    font-size: 16pt;
    text-shadow: 0 0 8px #ffcc99;
}

